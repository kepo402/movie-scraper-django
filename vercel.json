{
  "version": 2,
  "builds": [
    {
      "src": "custommoviesite/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.1" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/favicon.ico", "dest": "/static/favicon.ico" },
    { "src": "/(.*)", "dest": "custommoviesite/wsgi.py" }
  ],
  "outputDirectory": "staticfiles"
}
